#include "mt7628an_tplink_re200.dtsi"

/ {
	compatible = "tplink,re200-v4", "mediatek,mt7628an-soc";
	model = "TP-Link RE200 v4";

	leds {
		wifi_red {
			label = "red:wifi";
			gpios = <&gpio 40 GPIO_ACTIVE_LOW>;
		};
	};
};

&led_power {
	gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
};
